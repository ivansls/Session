<Window x:Class="UP02.MasterWindowo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP02"
        mc:Ignorable="d"
        Title="MasterWindowo" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.1*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Frame x:Name="FrameM" Grid.Row="1" Grid.ColumnSpan="3" NavigationUIVisibility="Hidden"/>
        <Button Content="Изделия" Click="Button_Click"/>
        <Button Content="Ингридиенты" Grid.Column="1" Click="Button_Click_1"/>
        <Button Content="Украшения" Grid.Column="2" Click="Button_Click_2"/>
    </Grid>
</Window>
